def generate_prime_numbers(n):
    sieve = [True] * (n+1)
    sieve[0] = sieve[1] = False

    prime_numbers = []
    for p in range(2, n+1):
        if sieve[p]:
            prime_numbers.append(p)
            for i in range(p*p, n+1, p):
                sieve[i] = False
    
    return prime_numbers

